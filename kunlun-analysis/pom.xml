	</parent>
	<artifactId>kunlun-analysis</artifactId>
	<packaging>jar</packaging>

	<name>kunlun-analysis</name>
	<url>http://www.hansight.com</url>

	<repositories>
		<repository>
			<id>local</id>
			<url>http://172.16.219.6:8081/nexus/content/groups/public/</url>
		</repository>
		</repositories>


	<properties>
		<hadoop.version>2.4.0.2.1.4.0-632</hadoop.version>
